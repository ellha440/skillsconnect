<%= simple_form_for(@employee) do |f| %>
  <%= f.error_notification %>

  <div  class="form-inputs insert-table">

    <%= f.input :firstname %>
    <%= f.input :lastname %>
    <%= f.input :suburb %>
    <%= f.input :phonenumber %>
    <%= f.input :email %>
    <%= f.input :origin %>
    <%= f.input :age %>
    <%= f.input :gender %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
